<script>
function getCookie(key) {
var keyValue = document.cookie.match('(^|;) ?' + key + '=([^;]*)(;|$)');
return keyValue ? keyValue[2] : null;
}
jq.urlParam = function(name){
var results = new RegExp('[\?&]' + name + '=([^&#]*)').exec(window.location.href);
if (results==null){
return null;
}
else{
return results[1] || 0;
}
}
function showBlogInfo() {
jq.fancybox({
type : 'inline',
arrows : false,
closeBtn : true,
content : jq('#blogInfo'),
padding : '0',
wrapCSS : 'info-layer',
autoCenter : true,
autoResize : true,
helpers: {
overlay: {
locked: true
}
}
});
}
var isBlogRedir = false;
var myTimeout;
if(jq.urlParam("r") == "blog") {
isBlogRedir = true;
}
function checkBlogRedirect() {
console.log("checkBlogRedirect-Aufruf");
var cookiePolicy = getCookie('cookie_policy');
if(dObj.helpers.cookieEnabled()){
if(cookiePolicy && isBlogRedir){
showBlogInfo();
} else if(!cookiePolicy && isBlogRedir){
myTimeout = setTimeout(checkBlogRedirect, 2000);
}
} else if (isBlogRedir){
showBlogInfo();
}
}
jq(document).ready(function($){
checkBlogRedirect();
});
</script>
